{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "out",
		"sourceMap": true,
		"noImplicitAny": true,
		"strictNullChecks": false,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"typeRoots": [
			"typings/globals",
			"typings/modules"
		]
	},
	"exclude": [
		"node_modules",
		"out"
	]
}